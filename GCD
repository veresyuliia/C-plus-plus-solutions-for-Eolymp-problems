#include<iostream>

using namespace std;

long long gcd(long long a,long long b)
{
    if(b>0)
    {
        return gcd(b,a%b);
    }
     return a;
}
int main()
{
    int n,m=0;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int a;
        cin>>a;
        m=gcd(m,a);
    }
    cout<<m;
    return 0;
}

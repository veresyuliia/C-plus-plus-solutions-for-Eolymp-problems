#include <iostream>
#include<vector>
#include<algorithm>

using namespace std;

 int dp[1000005];

void solve()
{
    int n;
    while(cin>>n){
        dp[1]=0;
        dp[2]=1;
        dp[3]=1;
        dp[4]=2;
        for(int i=5;i<=n;i++){
            dp[i]=dp[i-1]+1;
            if(i%3==0){
                dp[i]=min(dp[i],dp[i/3]+1);
            }
            if(i%2==0){
                dp[i]=min(dp[i/2]+1,dp[i]);
            }
        }
        cout<<dp[n]<<endl;
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    long long t=1;
    while(t--)
    {
        solve();
    }

    return 0;
}
